{
    "name": "Session Manager",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Keep track of sessions",
    "icons": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png",
        "180": "icons/favicon-180x180.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ]
}